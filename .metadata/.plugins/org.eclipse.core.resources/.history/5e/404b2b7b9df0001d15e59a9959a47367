package programers;

public class CtrlZ {

	public static void main(String[] args) {
		
		String s = "10 Z 20 Z 1";
		String[] arrStr = s.split(" ");
		
		int answer = 0;
		
		for(int i=0; i<arrStr.length; i++) {			
			if(arrStr[i].equals("Z")) {
				answer -= Integer.parseInt(arrStr[i-1]);
			} else {
				answer += Integer.parseInt(arrStr[i]);
			}
		}
			
			
			
			

	}

}
