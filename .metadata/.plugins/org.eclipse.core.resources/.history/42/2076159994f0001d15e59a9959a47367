package programers;

public class CtrlZ {

	public static void main(String[] args) {
		
		String s = "10 Z 20 Z 1";
		String[] arrStr = s.split(" ");
		
		int[] arrInt = new int[s.length()];
		
		int sum = 0;
		
		for(int i=0; i<arrInt.length; i++) {			
			if(arrStr[i].equals("Z")) {
				sum -= arrInt[i-1];
			} else {
				arrInt[i] = Integer.parseInt(arrStr[i]);
				sum += arrInt[i];
			}
		}
			
			
			
			

	}

}
